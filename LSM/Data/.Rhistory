sdValues <- apply(trainingX,2,sd)
for(i in 1:Nvar) {
trainingX[,i] <- scale(trainingX[,i],center=meanValues[i],scale = sdValues[i])
testX[,i] <- scale(testX[,i],center=meanValues[i],scale = sdValues[i])
}
