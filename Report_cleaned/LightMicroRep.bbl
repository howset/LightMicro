    \field{title}{A guide to light-sheet fluorescence microscopy for multiscale
  imaging}
    \verb{url}
    \verb https://www.nature.com/articles/nmeth.4224#citeas
    \endverb
    \field{volume}{14}
    \field{journaltitle}{Nature Methods}
    \field{year}{2017}
  \endentry

